#include<stdio.h>

main(){

int DDD;

printf("Informe o o DDD:");
scanf("d%", &DDD);

switch(DDD){
  case 61:
  printf("Este DDD é de Brasília");
  break;
  
case 71:
  printf("Este DDD é de Salvador");
  break;

case 11:
  printf("Este DDD é de São Paulo");
  break;

case 21:
  printf("Este DDD é de Rio Janeiro");
  break;

case 32:
  printf("Este DDD é de Juiz de Fora");
  break;

case 19:
  printf("Este DDD é de Campinas");
  break;

case 27:
  printf("Este DDD é de Vitória");
  break;

case 31:
  printf("Este DDD é de Belo Horizonte");
  break;

default:
  printf("Cidade sem identificação");
  break;}
}


  
